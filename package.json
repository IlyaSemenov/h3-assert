{
  "name": "h3-assert",
  "type": "module",
  "version": "1.1.2",
  "packageManager": "pnpm@9.14.2",
  "description": "Assert helpers for h3",
  "author": "Ilya Semenov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/h3-assert"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "!src/**/*.test.*",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint --fix .",
    "prepare": "husky",
    "prepublishOnly": "pnpm build",
    "test": "vitest run"
  },
  "peerDependencies": {
    "h3": "^1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@ilyasemenov/eslint-config": "^1.0.1",
    "eslint": "^9.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "tsconfig-vite-node": "^1.1.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.5"
  }
}
